Q2---------------------------------------------------------------

function reverse(){

var str = "Guneshwari"
var rev_str="";
for(var i = str.length-1;i>=0;i--)
{
rev_str = rev_str+str[i];
}
return rev_str;
}
reverse();
"irawhsenuG"


